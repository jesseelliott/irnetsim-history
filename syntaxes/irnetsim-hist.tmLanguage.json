{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "IrNetSim History Syntax Highlighter",
  "scopeName": "text.irnetsim-hist",
  "patterns": [
    {
      "name": "log.keywords1.hist",
      "match": "\\b(Configured|requested|Requesting|Sending|started|creating|destroying|Releasing|releaseResources|Examining|Submitting|Duplicate|Ignoring|ignored|Moving|downgrade|downgraded|Terminating|Flushing|bm: ?[1-4]?\\d|sb: ?[1-3]?\\d|ts: ?[1-4]|fa:1 2|fa:3 4|fa:5 6|fa:7 8|fa: ?C8|fa: ?[1-8])\\b"
    },
    {
      "name": "log.keywords2.hist",
      "match": "\\b(call|handoff|Opened|Found|true|false|on|off|version|LCID|lcid|RFN|SV[0-9]*|Traffic[0-9]*)\\b"
    },
    {
      "name": "log.keywords3.hist",
      "match": "\\b(DownlinkMaintenance|BBDownlinkMaintenance|BBUplinkMaintenance|UplinkMaintenance|HandoffCandidate|Null|ACCHL_SEGMENT|IAL_SEGMENT|LBP_CANDIDATE_BEAM_LIST|LL_ENCAPSULATED_MSG|VOX_VOICE|DATA_MSG|BB_ISU_V5_STATUS_REPORT|BB_ISU_V6_STATUS_REPORT|BB_MAC_MUX_SDU|EMPTY_PAYLOAD)\\b"
    },
    {
      "name": "log.keywords4.hist",
      "match": "\\b(DownlinkSyncReport|SyncTrafficSwitch|BcastChannelAssignment|SyncCheck|ACCHL|HandoffRequest|BBHandoffRequest|BBHandoffToAnywhere|HandoffResponse|BBHandoffDenied|NettedHandoffRequest|BBHandoffReducedFrameNumber|BCAST_CHANNEL_ASSIGNMENT|ACCHL_RESET|BTP_SEGMENT|CC_ALERTING|CC_CALL_CONFIRMED|CC_CALL_PROCEEDING|CC_CONNECT|CC_CONNECT_ACK|CC_DISCONNECT|CC_RELEASE|CC_RELEASE_COMPLETE|CC_SETUP|ILAP_TRIGGER_GEOLOCATION|ILAP_RR|ILAP_RNR|ILAP_REJ|ILAP_SABME|ILAP_DM|ILAP_UI|ILAP_DISC|ILAP_UA|ILAP_XID|RAW_ITM|IST_TELEMETRY|PFC_TELEMETRY|ISUP_ADDRESS_COMPLETE|ISUP_INITIAL_ADDRESS|ISUP_RELEASE|LBP_ACQUISITION_REQUEST|LBP_RING_ALERT_INDICATION|LL_ACCESS_DECISION_NOTIFICATION|LL_ASSIGNMENT_COMPLETE|LL_ASSIGNMENT_REQUEST|LL_CHANGE_OUTBOUND_SIGNALING|LL_CHANNEL_ESTABLISHED|LL_CHANNEL_OPTIONS|LL_CHANNEL_REASSIGNMENT_ORDER|LL_GEOLOCATION_BURST_REQUEST|LL_GEOLOCATION_DOWNLINK|LL_HANDOFF_COMPLETE_NOTIFICATION|LL_HOME_GW_ACCESS_REQUEST|LL_INTER_SV_HANDOFF_ORDER_TO_ISU|LL_INTER_SV_HANDOFF_ORDER_TO_SV|LL_MESSAGE_DELIVERY_ORDER|LL_MESSAGE_DELIVERY_ORDER_REJECT|LL_PAGING_RESPONSE|LL_PATHEND_CHANNEL_ASSIGNMENT|LL_PATHEND_CHANNEL_REQUEST|LL_REDIRECT|LL_RELEASE_WARNING|LL_RELEASE|LL_RELEASE_REQUEST|LL_RELEASE_CHANNEL|LL_RELEASE_DONE|LL_REMOTE_SV_HANDOFF_NOTIFICATION|LL_RING_ALERT_REQUEST|LL_SBD_ACCESS_DECISION_NOTIFICATION|LL_SBD_GW_ACCESS_REQUEST|LL_SBD_PARAMETER_LIST_VERSION_QUERY|LL_SBD_DATA_SET_SUBGROUP_VERSION_QUERY|LL_SBD_DATA_SET_DOWNLOAD|LL_SBD_DOWNLOAD_COMPLETE|LL_SBD_PARAMETER_LIST_VERSION_RESPONSE|LL_SBD_DOWNLOAD_RESPONSE|LL_SBD_DATA_SET_SUBGROUP_VERSION_RESPONSE|LL_SBD_SV_ACCESS_REQUEST|LL_SV_ACCESS_REQUEST|LL_SV_FORWARDED_HANDOFF_REQUEST|LL_X_MESSAGE_DELIVERY_ORDER|LL_X_MESSAGE_DELIVERY_ORDER_REJECT|MM_AUTHENTICATION_REQUEST|MM_AUTHENTICATION_RESPONSE|MM_LOCATION_UPDATE_REQUEST|MM_LOCATION_UPDATE_ACCEPT|MM_TMSI_REALLOCATION_COMPLETE|MM_SERVICE_ACCEPT|MM_SERVICE_REQUEST|NAP_FORWARD_RELEASE|NAP_RELEASE_COMPLETE|SVRT_DB_UPDATE|SVRT_DB_RECOVERY_REQUEST|SVRT_DB_REQUEST|SVRT_DB_RESPONSE|SVRT_DB_CHANGE_WILDCARD|HPL_DATA_MSG|LL_BB_SV_ACCESS_REQUEST|LL_BB_HOME_GW_ACCESS_REQUEST|LL_BB_SV_ACCESS_DECISION_NOTIFICATION|LL_BB_ISU_ACCESS_DECISION_NOTIFICATION|LL_BB_SV_ADN_ACCEPTED|LL_BB_GW_ADN_ACCEPTED|LL_BB_PATHEND_CHANNEL_REQUEST|LL_BB_PATHEND_CHANNEL_ASSIGNMENT|LL_BB_CHANNEL_ASSIGNMENT|LL_BB_CHANNEL_REUSE_RELEASE_WARNING|LL_BB_GW_FORWARDED_HO_REQUEST|LL_BB_INTER_SV_HO_ORDER|LL_BB_HO_COMPLETE_NOTIFICATION|LL_BB_LOSING_SV_HO_COMPLETE_NOTIFICATION|LL_BB_RELEASE_SV|LL_BB_RELEASE_CONNECTION|LL_BB_RELEASE_DONE|LL_BB_TERMINATE_CONNECTION|BB_ISU_BW_CHANGE_ORDER|LL_STATIC_TCH_PATHEND_CHANNEL_REQUEST|LL_STATIC_TCH_CHANNEL_RELEASE_REQUEST|LL_STATIC_TCH_PATHEND_CHANNEL_ASSIGNMENT|LL_STATIC_TCH_RELEASE_CONNECTION|AW_RESOURCE_REQUEST|AW_RELEASE_REQUEST|AW_RESOURCE_RESPONSE|AW_RELEASE_RESPONSE|AW_ENCRYPTION_KEY_UPLOAD|AW_ENCRYPTION_KEY_UPLOAD_RESPONSE|NETTED_CHANNEL_ASSIGNMENT|LL_NETTED_ACCESS_REQUEST|LL_NETTED_RELEASE_CHANNELS|LL_NETTED_ISU_CONFIG|LL_NETTED_RELEASE|LL_NETTED_GW_CONFIG_SETUP|LL_NETTED_GW_CONFIG_RELEASE|LL_NETTED_REGISTRATION|LBP_NETTED_REPEATER_CONFIG|LBP_NETTED_ISU_RELEASE|LBP_NETTED_ISU_BROADCAST_PLI|TACTICAL_NET_REQUEST|LL_TACTICAL_NET_SETUP|L1MP_NETTED_REG_ATTACH_REQUEST|L1MP_NETTED_REG_ATTACH_CONFIRM|L1MP_NETTED_CONNECT_REQUEST|L1MP_NETTED_TMSI_RESPONSE|L1MP_NETTED_REG_AUTH_REQUEST|L1MP_NETTED_REG_AUTH_CONFIRM|L1MP_NETTED_REG_COMPLETE_REQUEST|L1MP_NETTED_REG_COMPLETE_CONFIRM|L1MP_NETTED_CONNECT_RELEASE_REQUEST|L1MP_NETTED_SHORT_AMS_WRITE_REQUEST|L1MP_NETTED_THEATER_ACCESS_REQUEST|L1MP_HANDOFF_CANDIDATE_LIST_IND|L2_NETTED_AMS_OPEN_REQUEST|L2_NETTED_AMS_DATA_REQUEST|L2_NETTED_AMS_DATA_RESPONSE|L2_NETTED_AMS_CLOSE_REQUEST|L2_NETTED_AMS_OPEN_CONFIRM|L2_NETTED_AMS_DATA_CONFIRM|L2_NETTED_AMS_DATA_INDICATION|L2_NETTED_AMS_CLOSE_INDICATION)\\b"
    },
    {
      "name": "invalid.hist",
      "match": "\\b(warning|WARNING|Warning|error|Error|ERROR|Fatal|FATAL|fatal|devAssert|Failed|failed|Blocked|Dropped|Faded|Lost|corrupted|corruption)\\b"
    },
    {
      "name": "constant.numeric.hist",
      "match": "\\d"
    },
    {
      "name": "comment.line.double-slash.hist",
      "match": "//.*"
    },
    {
      "name": "keyword.operator.hist",
      "match": "[\\+\\-\\*\\/\\%\\!\\^\\|\\(\\)\\{\\}\\[\\]\\<\\>\\:\\=\\,\\;]"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "strings": {
      "name": "string.quoted.hist",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.hist",
          "match": "\\\\."
        }
      ]
    }
  }
}